{
    "version": 2,
    "builds": [
        {
          "src": "package.json",
          "use": "@vercel/node"
        }
      ],
      
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/" 
      }
    
    ],
    "install": {
        "include": ["package.json"],
        "additionalSteps": ["npm i --legacy-peer-deps"]
      }
  
  }
  